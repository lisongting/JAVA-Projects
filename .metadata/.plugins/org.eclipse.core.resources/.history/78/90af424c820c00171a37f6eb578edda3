package nowcode;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Stack;

public class Main8 {
    public static boolean isContinuous(int [] numbers) {
    	Arrays.sort(numbers);
    	int zero = 0;
    	int min,max;
    	for(int i=0;i<numbers.length;i++){
    		if(numbers[i]==0){
    			zero++;
    		}
    	}
    	switch(zero){
    		case 0:
    			if(4==(numbers[4]-numbers[0])){
            		return true;
            	}
    			break;
    		case 1:
    			min = numbers[1];
    			max = numbers[4];
    			if(max-min<=4){
    				return true;
    			}
    			break;
    		case 2:
    			min = numbers[2];
    			max = numbers[4];
    			if(max-min<=4){
    				return true;
    			}
    			break;
    		case 3:
    			min = numbers[3];
    			max = numbers[4];
    			if(max-min<=4){
    				return true;
    			}
    			break;
    		default:
    			return false;
    	}
    	
    	return false;
    }
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<>();
		int [] test = new int[]{0,3,1,0,7};
		System.out.println(isContinuous(test));
	}

}
